  },
  "dependencies": {
    "@types/node": "^20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "^18.2.7",
    "exifr": "^7.1.3",
    "image-size": "^1.0.2",
    "photoswipe": "^5.3.8",
    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-photoswipe-gallery": "^2.2.7",
    "sass": "^1.65.1",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "web-vitals": "^3.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
